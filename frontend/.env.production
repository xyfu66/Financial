# Production environment configuration
VITE_APP_TITLE=個人財務管理システム
VITE_APP_DESCRIPTION=Personal Financial Management System for Japanese Individual Business Owners

# API Configuration
VITE_API_BASE_URL=https://your-domain.com/api/v1
VITE_API_TIMEOUT=31000

# Application Configuration
VITE_DEFAULT_LOCALE=ja
VITE_SUPPORTED_LOCALES=ja,en

